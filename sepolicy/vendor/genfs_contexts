# Display
genfscon sysfs /devices/platform/soc/5e00000.qcom,mdss_mdp/idle_state    u:object_r:vendor_sysfs_graphics:s0
genfscon sysfs /devices/platform/soc/soc:qcom,dsi-display                u:object_r:vendor_sysfs_graphics:s0

# Health
genfscon sysfs /class/power_supply/battery/capacity                                                 u:object_r:sysfs_battery_supply:s0

# UFS
genfscon sysfs /devices/platform/soc/4804000.ufshc/clkgate_enable               u:object_r:vendor_sysfs_scsi_host:s0

# Wakeup source stats
genfscon sysfs /devices/platform/soc/1c40000.qcom,spmi/spmi-0/spmi0-02/1c40000.qcom,spmi:qcom,pmi632@2:qcom,qpnp-smb5/dual_role_usb/otg_default/wakeup    u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/soc/4a8c000.i2c/i2c-1/1-0028/wakeup                                                                                      u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/virtual/input/input1/wakeup                                                                                                       u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/virtual/input/input2/wakeup                                                                                                       u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/virtual/wakeup/wakeup                                                                                                             u:object_r:sysfs_wakeup:s0
